{
  "name": "cloudydesktop",
  "version": "1.0.0",
  "description": "Mirror local folder to a Cloudinary folder",
  "main": "main.js",
  "scripts": {
    "test": "electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "pkgbuild": "pkgbuild --component out/cloudydesktop-darwin-x64/cloudydesktop.app --scripts scripts --sign 'Developer ID Installer: Cloudinary LTD (3TR999VVAS)' --install-location /Applications out/cloudypkg-signed.pkg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudinary-labs/cloudydesktop.git"
  },
  "keywords": [
    "cloudinary",
    "desktop"
  ],
  "author": "Hadar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cloudinary-labs/cloudydesktop/issues"
  },
  "homepage": "https://github.com/cloudinary-labs/cloudydesktop#readme",
  "dependencies": {
    "child_process": "^1.0.2",
    "chokidar": "^3.4.0",
    "dotenv": "^10.0.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^5.2.0",
    "keytar": "^6.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "electron": "^9.3.2"
  },
  "config": {
    "forge": "./forge.config.js"
  }
}
